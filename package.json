{
  "name": "headless-tools-cms",
  "version": "1.0.0",
  "description": "企业级无头CMS后端项目，集成完整的开源高级功能栈",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "serve": "cross-env NODE_ENV=production node dist/server.js",
    "start": "npm run build && npm run serve",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "headless-cms",
    "payload-cms",
    "open-source",
    "analytics",
    "monitoring",
    "email-marketing",
    "seo",
    "enterprise"
  ],
  "author": "无头工具站开发团队",
  "license": "MIT",
  "dependencies": {
    "payload": "^2.8.0",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.5.1",
    "@payloadcms/richtext-lexical": "^1.5.2",
    "@payloadcms/plugin-seo": "^2.3.2",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-form-builder": "^1.2.3",
    "@payloadcms/plugin-cloud-storage": "^1.1.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "sharp": "^0.32.6",
    "nodemailer": "^6.9.7",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "rate-limiter-flexible": "^4.0.1",
    "validator": "^13.11.0",
    "slugify": "^1.6.6",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",

    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.14",
    "@types/validator": "^13.11.7",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/headless-tools-cms.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/headless-tools-cms/issues"
  },
  "homepage": "https://github.com/your-username/headless-tools-cms#readme"
}
